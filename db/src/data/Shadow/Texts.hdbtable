COLUMN TABLE "shadow::UtilShadow.Texts" (
     "TEXTID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 1000000000 INCREMENT BY 1 MAXVALUE 1999999999) NOT NULL COMMENT 'Text ID',
	 "LANGUAGE" NVARCHAR(1) COMMENT 'Language',
	 "ISOLANGUAGE" NVARCHAR(2) COMMENT 'ISO Language Key',
	 "TEXT" NVARCHAR(1024) COMMENT 'Text',
	 PRIMARY KEY ("TEXTID")) 
	 COMMENT 'Long Texts'
	 UNLOAD PRIORITY 5 AUTO MERGE 