COLUMN TABLE "shadow::UtilShadow.Constants" (
     "DOMAIN" NVARCHAR(30) NOT NULL COMMENT 'Domain Name',
	 "FIXEDVALUE" NVARCHAR(30) NOT NULL COMMENT 'Fixed Value',
	 "LANGUAGE" NVARCHAR(1) NOT NULL COMMENT 'Language',
	 "DESCRIPTION" NVARCHAR(60) COMMENT 'Description',
	 PRIMARY KEY ("DOMAIN", "FIXEDVALUE", "LANGUAGE")) 
	 COMMENT 'Constants - like ABAP Domains'
	 UNLOAD PRIORITY 5 AUTO MERGE 
