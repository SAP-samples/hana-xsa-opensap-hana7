COLUMN TABLE "Util.Notes" (
     "NOTEID" NVARCHAR(10) NOT NULL COMMENT 'Note ID',
	 "LINKID" NVARCHAR(10) COMMENT 'Link ID',
	 "AUTHOR" NVARCHAR(30) COMMENT 'Note Author',
	 "TEXT" NVARCHAR(255) COMMENT 'Text',
	 "CREATEDAT" DATE CS_DAYDATE COMMENT 'Create Date',
	 "THUMBNAIL" NVARCHAR(255) COMMENT 'Thumbnail Image URL',
	 PRIMARY KEY ("NOTEID")) 
	 COMMENT 'Notes'
	 UNLOAD PRIORITY 5 AUTO MERGE
