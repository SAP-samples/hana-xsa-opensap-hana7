VIEW "Util.NotesView" COMMENT 'Notes View' ( 
     "NOTEID",
	 "Author",
	 "Text",
	 "CREATEDAT",
	 "Thumbnail",
	 "LINKID" ) AS SELECT
	 "Notes_$0"."NOTEID" ,
	 "Notes_$0"."AUTHOR" AS "Author" ,
	 "Notes_$0"."TEXT" AS "Text" ,
	 "Notes_$0"."CREATEDAT" ,
	 "Notes_$0"."THUMBNAIL" AS "Thumbnail" ,
	 "Notes_$0"."LINKID" 
FROM "Util.Notes" AS "Notes_$0" 
